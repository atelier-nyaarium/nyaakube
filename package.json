		"@types/express-session": "^1.17.10",
		"@types/jest": "^29.5.11",
		"@types/lodash": "^4.14.202",
		"@types/multer": "^1.4.11",
		"@types/react": "^18.2.45",
		"@types/react-image-gallery": "^1.2.4",
		"@types/saslprep": "^1.0.3",
		"@types/uuid": "^9.0.7",
		"@typescript-eslint/eslint-plugin": "^6.17.0",
		"@typescript-eslint/parser": "^6.16.0",
		"argon2": "^0.31.2",
		"connect-pg-simple": "^9.0.1",
		"dotenv": "^16.3.1",
		"eslint": "^8.56.0",
